{
    "name": "mercato/project",
    "description": "Sistema de compra e venda de produtos",
    "type": "project",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Mercato\\": "app/",
            "Utility\\": "Utility/"
        }
    },
    "require": {
        "php": ">=8.0",
        "dompdf/dompdf": "^2.0"

    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable"
}